{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-simple-text-diff": {
      "projectType": "library",
      "root": "projects/ngx-simple-text-diff",
      "sourceRoot": "projects/ngx-simple-text-diff/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-simple-text-diff/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-simple-text-diff/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-simple-text-diff/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "ngx-simple-text-diff-showcase": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-simple-text-diff-showcase",
      "sourceRoot": "projects/ngx-simple-text-diff-showcase/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ngx-simple-text-diff-showcase",
            "index": "projects/ngx-simple-text-diff-showcase/src/index.html",
            "browser": "projects/ngx-simple-text-diff-showcase/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/ngx-simple-text-diff-showcase/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ngx-simple-text-diff-showcase/src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "projects/ngx-simple-text-diff-showcase/src",
                "output": "/"
              }
            ],
            "styles": [
              "projects/ngx-simple-text-diff-showcase/src/styles.scss"
            ],
            "scripts": ["./node_modules/diff/dist/diff.min.js"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngx-simple-text-diff-showcase:build:production"
            },
            "development": {
              "buildTarget": "ngx-simple-text-diff-showcase:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        }
      }
    }
  }
}
